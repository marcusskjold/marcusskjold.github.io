@book{bratich2008ConspiracyPanicsPolitical,
  title = {Conspiracy Panics: Political Rationality and Popular Culture},
  shorttitle = {Conspiracy Panics},
  author = {Bratich, Jack Z},
  year = {2008},
  publisher = {{State University of New York Press}},
  address = {{Albany, NY}},
  url = {https://search.ebscohost.com/login.aspx?direct=true&scope=site&db=nlebk&db=nlabk&AN=220051},
  urldate = {2022-08-10},
  abstract = {"While most other works focus on conspiracy theories, this book examines conspiracy panics, or the anxiety over the phenomenon of conspiracy theories. Jack Z. Bratich argues that conspiracy theories are portals into the major social issues defining U.S. and global political culture. These issues include the rise of new technologies, the social function of journalism, U.S. race relations, citizenship and dissent, globalization, biowarfare and biomedicine, and the shifting positions within the Left. Using a Foucauldian governmentality analysis, Bratich maintains that conspiracy panics contribute to a broader political rationality, a (neo)liberal strategy of governing at a distance through the use of reason. He also explores the growing popularity of 9/11 conspiracy research in terms of what he calls the "sphere of legitimate dissensus." Conspiracy Panics concludes that we are witnessing a new fusion of culture and rationality, one that is increasingly shared across the political spectrum."--Jacket},
  isbn = {978-1-4356-3210-3},
  langid = {english},
  annotation = {OCLC: 212092979},
  file = {/Users/moonqueenstudio/Library/Zotero library/storage/8TYI7P7W/Bratich - 2008 - Conspiracy panics political rationality and popul.pdf}
}

