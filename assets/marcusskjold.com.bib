@article{karel2016NotesSpaceConsciousness,
  title = {Notes on `{{Space}} of {{Consciousness}} ({{Chidambaram}}, {{Early Morning}})'},
  author = {Karel, Ernst},
  year = {2016},
  month = dec,
  journal = {Anthrovision},
  number = {4.2},
  issn = {2198-6754},
  doi = {10.4000/anthrovision.2383},
  url = {http://journals.openedition.org/anthrovision/2383},
  urldate = {2021-09-07},
  abstract = {The small city of Chidambaram, Tamilnadu, is home to an ancient and massive temple complex, at the center of which is the famous site where \'Siva, as Na\d{t}ar\=aja, the Lord of the Dance, performs the Cosmic Dance of Bliss, the \=ananda t\=a\d{n}\d{d}ava, in the Hall of Consciousness. The temple is specifically associated with \=ak\=a\'sa (aether, space, or sky), the fifth of the five elements which is known as the medium of sound, and \'Siva Na\d{t}ar\=aja is celebrated here with sonorous practices that include the ringing of two large temple bells. The sound piece published here, `Space of consciousness (Chidambaram, early morning)', represents the situatedness of the temple complex within the city, the main shrine at the center of the temple, and finally the immersive sonic and semiotic resonances of the large bells. The accompanying text describes a network of embodied meanings implicated experientially in sound.},
  keywords = {Audio,Hinduism,India,Read,Sonic ethnography,Sound},
  file = {/Users/moonqueenstudio/Library/Mobile Documents/com~apple~CloudDocs/Marcus iCloud/Medier/Tidsskriftsartikel/Karel_2016_Notes on ‘Space of Consciousness (Chidambaram, Early Morning)’.mp3;/Users/moonqueenstudio/Library/Mobile Documents/com~apple~CloudDocs/Marcus iCloud/Medier/Tidsskriftsartikel/Karel_2016_Notes on ‘Space of Consciousness (Chidambaram, Early Morning)’.pdf}
}

